{
  "name": "opulence/cache",
  "description": "The Opulence cache library",
  "keywords": [
    "PHP",
    "cache",
    "Redis",
    "Memcached"
  ],
  "authors": [
    {
      "name": "David Young",
      "email": "dave@opulencephp.com",
      "role": "Architect"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Opulence\\Cache\\": ""
    },
    "exclude-from-classmap": [
        "/Tests/"
    ]
  },
  "require": {
    "php": "^7.1",
    "ext-memcached": "*"
  },
  "suggest": {
    "opulence/memcached": "Required for Memcached cache",
    "opulence/redis": "Required for Redis cache"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "minimum-stability": "dev"
}