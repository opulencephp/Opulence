{
  "name": "opulence/redis",
  "description": "The Opulence Redis library",
  "keywords": [
    "PHP",
    "redis",
    "nosql"
  ],
  "authors": [
    {
      "name": "David Young",
      "email": "dave@opulencephp.com",
      "role": "Architect"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Opulence\\Redis\\": ""
    },
    "exclude-from-classmap": [
        "/Tests/"
    ]
  },
  "require": {
    "php": "^7.3 || ^8.0"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "phpredis",
        "version": "3.0.*",
        "source": {
          "url": "https://github.com/phpredis/phpredis.git",
          "type": "git",
          "reference": "3.0.0"
        }
      }
    }
  ],
  "suggest": {
    "phpredis": "Required for PHPRedis support",
    "predis/predis": "Required for Predis support"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  },
  "minimum-stability": "dev"
}
