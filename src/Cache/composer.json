{
    "name": "opulence/cache",
    "description": "The Opulence cache library",
    "keywords": [
        "PHP",
        "cache",
        "Redis",
        "Memcached"
    ],
    "authors": [
        {
            "name": "David Young",
            "email": "dave@opulencephp.com",
            "role": "Architect"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Opulence\\Cache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opulence\\Cache\\Tests\\": "tests"
        }
    },
    "require": {
        "ext-memcached": "*",
        "ext-redis": "*",
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.14",
        "phpunit/phpunit": "~9.0"
    },
    "suggest": {
        "ext-redis": "Required for PHPRedis support"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
